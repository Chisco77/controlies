.qq-uploader {
    min-height : 100px;

    max-height : 200px;
    overflow-y : auto;
}


.qq-upload-button {
    /*    heigth: 10px;
	  width: 80px;
	  margin-top : 4px;
	  text-align : center;
	  background-color: white;
	  */
    border-style: none;
}

.qq-upload-button-hover {}
.qq-upload-button-focus {}

.qq-upload-drop-area {
    position:absolute; top:0; left:0; width:100%; height:100%; min-height: 70px; z-index:2;
    background:#FF9797; text-align:center; 
}
.qq-upload-drop-area span {
    display:block; 
    position:absolute; 
    top: 50%; 
    width:100%;
    margin-top:-8px; 
    font-size:16px;
}
.qq-upload-drop-area-active {
    background:#FF7171;
}

.qq-upload-list { 
    padding:0;
    margin-top : 10px;
    border-top : 1px solid #333;
    overflow-x: hidden;
    list-style-type:none;
}

.qq-upload-list li { 
    overflow-x: hidden;
    margin:0; padding:0; 
    line-height:20px; 
    font-size:12px;
    display:inline;    
}

.qq-upload-file, .qq-upload-spinner, .qq-upload-size, .qq-upload-cancel, .qq-upload-failed-text {
    margin-right: 10px;
}

.qq-upload-cancel {
    color : red;
    text-decoration:none;
}

.qq-upload-file {}
.qq-upload-spinner {display:inline-block; background: url("../wait2.gif"); width:15px; height:15px; vertical-align:text-bottom;}

.qq-upload-size{

}

.qq-upload-size,.qq-upload-cancel {font-size:11px;}

.qq-upload-failed-text {display:none;}
.qq-upload-fail .qq-upload-failed-text {display:inline;}


.list-down{
    width : 200px;
    height : 200px;
    background-color : black;
    z-index : 9999;
}
